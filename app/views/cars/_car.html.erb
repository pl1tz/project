<div id="<%= dom_id car %>">
  <p>
    <strong>Model:</strong>
    <%= car.model_id %>
  </p>

  <p>
    <strong>Brand:</strong>
    <%= car.brand_id %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= car.year %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= car.price %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= car.description %>
  </p>

  <p>
    <strong>Color:</strong>
    <%= car.color_id %>
  </p>

  <p>
    <strong>Body type:</strong>
    <%= car.body_type_id %>
  </p>

  <p>
    <strong>Engine type:</strong>
    <%= car.engine_type_id %>
  </p>

  <p>
    <strong>Gearbox type:</strong>
    <%= car.gearbox_type_id %>
  </p>

  <p>
    <strong>Drive type:</strong>
    <%= car.drive_type_id %>
  </p>


</div>
